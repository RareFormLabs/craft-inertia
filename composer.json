{
  "name": "rareform/craft-inertia",
  "description": "Inertia.js adapter for Craft CMS",
  "version": "1.0.0-alpha.2",
  "type": "craft-plugin",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "craftcms/cms": "^5.0.0"
  },
  "require-dev": {
    "craftcms/generator": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "rareform\\inertia\\": "src/"
    }
  },
  "extra": {
    "handle": "inertia",
    "name": "Inertia",
    "developer": "RareForm",
    "documentationUrl": "https://github.com/rareformlabs/craft-inertia"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
